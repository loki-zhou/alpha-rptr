{
  "_enable_learner_api": false,
  "_enable_rl_module_api": false,
  "callbacks": "<class 'ray_callback.TradeMetricsCallbacks'>",
  "env": "TradingEnv2",
  "framework": "torch",
  "gamma": 0.99,
  "lambda": 0.95,
  "lr": 8e-06,
  "lr_schedule": [
    [
      0,
      0.1
    ],
    [
      1000,
      0.01
    ],
    [
      10000,
      0.001
    ],
    [
      100000,
      0.0001
    ],
    [
      1000000,
      1e-05
    ],
    [
      10000000,
      1e-06
    ],
    [
      100000000,
      1e-07
    ]
  ],
  "model": {
    "lstm_cell_size": 256,
    "lstm_use_prev_action": true,
    "lstm_use_prev_reward": true,
    "use_lstm": true
  },
  "num_gpus": 0,
  "num_sgd_iter": 16,
  "observation_filter": "MeanStdFilter",
  "vf_loss_coeff": 0.0001
}